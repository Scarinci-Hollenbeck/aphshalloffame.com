generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Ceremony {
  id       Int    @id @default(autoincrement())
  ceremony String
}

model ContactMember {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Member {
  id        Int    @id @default(autoincrement())
  name      String
  lastName  String
  class     String
  inducted  String
  biography String @db.VarChar(14000)
  slug      String @unique
}

model Years {
  id   Int    @id @default(autoincrement())
  year String @unique
}

model keepAwake {
  keepOn Int?
  id     Int  @id @unique(map: "id") @default(autoincrement())
}

model PageSEO {
  id                  Int    @id @default(autoincrement())
  name                String
  pageTitle           String
  pageMetaDescription String
  slug                String @unique
}
