generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model aphs_ceremony {
  id       Int    @id @default(autoincrement())
  ceremony String
}

model aphs_contactMember {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model aphs_member {
  id        Int    @id @default(autoincrement())
  name      String
  lastName  String
  class     String
  inducted  String
  biography String @db.VarChar(14000)
  slug      String @unique
}

model aphs_years {
  id   Int    @id @default(autoincrement())
  year String @unique
}

model aphs_pageSEO {
  id                  Int    @id @default(autoincrement())
  name                String
  pageTitle           String
  pageMetaDescription String
  slug                String @unique
}
